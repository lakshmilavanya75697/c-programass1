#include<stdio.h>
main()
{
	int number,r,sum=0,num;
	printf("enter the number:");
	scanf("%d",&number);
	num=number;
	while(num!=0)
	{
		r=num%10;
		sum=sum+(r*r*r);
		num=num/10;
		
	}
	if(sum==number)
	
		printf("%d is a armstrong number",number);
		
	
	else
	printf("%d is not a armstrong number",number);
}
